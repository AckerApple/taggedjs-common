{
  "scripts": {
    "save": "git add . && git commit -m \"patch\" && git push",
    "start": "run-p build:js:watch start:hmr",
    "start:hmr": "node tagged-hmr.js dir=\"./assets/js\"",
    "build": "run-s build:js build:bundle build:hmr",
    "build:bundle": "node runBundleScript.js",
    "build:hmr": "node hmr/runBundleScript.js",
    "build:js": "tsc -p tsconfig.json",
    "build:js:watch": "tsc -p tsconfig.json --watch"
  },
  "devDependencies": {
    "express": "^4.18.2",
    "npm-run-all": "^4.1.5",
    "serve-static": "^1.15.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "ws": "^8.15.1"
  },
  "dependencies": {
    "taggedjs": "github:ackerapple/taggedjs",
    "taggedjs-gateway": "github:ackerapple/taggedjs-gateway"
  }
}
